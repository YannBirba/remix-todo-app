{
  "name": "todo-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "vite build && vite build --ssr",
    "dev": "node ./server.mjs",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "prod": "pnpm run build && pnpm run start",
    "typecheck": "tsc",
    "generate": "drizzle-kit generate:sqlite",
    "push": "drizzle-kit push:sqlite",
    "update:latest": "pnpm update --latest && pnpm add @remix-run/express@latest @remix-run/node@latest @remix-run/react@latest && pnpm add -D @remix-run/dev@latest @remix-run/eslint-config@latest && pnpm add react@latest react-dom@latest && pnpm add -D remix-development-tools@pre",
    "update:canary": "pnpm update --latest && pnpm add @remix-run/express@nightly @remix-run/node@nightly @remix-run/react@nightly && pnpm add -D @remix-run/dev@nightly @remix-run/eslint-config@nightly && pnpm add react@canary react-dom@canary && pnpm add -D remix-development-tools@pre"
  },
  "dependencies": {
    "@epic-web/cachified": "^4.0.0",
    "@remix-run/express": "0.0.0-nightly-bf6c0a4-20231129",
    "@remix-run/node": "0.0.0-nightly-bf6c0a4-20231129",
    "@remix-run/react": "0.0.0-nightly-bf6c0a4-20231129",
    "better-sqlite3": "^9.1.1",
    "defer-if": "^1.0.5",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "isbot": "^3.7.1",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.294.0",
    "react": "18.3.0-canary-c17a27ef4-20231127",
    "react-aria": "^3.30.0",
    "react-aria-components": "1.0.0-rc.0",
    "react-dom": "18.3.0-canary-c17a27ef4-20231127",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remix-json-routes": "^0.1.1",
    "remix-utils": "^7.3.0",
    "static-path": "^0.0.4",
    "tiny-invariant": "^1.3.1",
    "valibot": "^0.21.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.19.0",
    "@remix-run/dev": "0.0.0-nightly-bf6c0a4-20231129",
    "@remix-run/eslint-config": "0.0.0-nightly-bf6c0a4-20231129",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.54.0",
    "react-refresh": "^0.14.0",
    "remix-development-tools": "3.5.0-rc4",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
