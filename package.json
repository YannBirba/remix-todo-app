{
  "name": "todo-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "vite build && vite build --ssr",
    "dev": "node ./server.mjs",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "prod": "pnpm run build && pnpm run start",
    "typecheck": "tsc",
    "generate": "drizzle-kit generate:sqlite",
    "push": "drizzle-kit push:sqlite",
    "update-remix:latest": "pnpm add @remix-run/{node,react,express}@latest && pnpm add -D @remix-run/{dev,eslint-config}@latest",
    "update-remix:nightly": "pnpm add @remix-run/{node,react,express}@nightly && pnpm add -D @remix-run/{dev,eslint-config}@nightly"
  },
  "dependencies": {
    "@remix-run/express": "^2.3.0",
    "@remix-run/node": "2.3.0",
    "@remix-run/react": "2.3.0",
    "better-sqlite3": "^9.1.1",
    "defer-if": "^1.0.5",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "isbot": "^3.7.1",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-aria": "^3.30.0",
    "react-aria-components": "1.0.0-rc.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-json-routes": "^0.1.1",
    "remix-utils": "^7.1.0",
    "static-path": "^0.0.4",
    "tiny-invariant": "^1.3.1",
    "valibot": "^0.20.1",
    "{node,react,express}@nightly": "link:@remix-run/{node,react,express}@nightly"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.18.3",
    "@remix-run/dev": "2.3.0",
    "@remix-run/eslint-config": "2.3.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/better-sqlite3": "^7.6.7",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.54.0",
    "react-refresh": "^0.14.0",
    "remix-development-tools": "3.5.0-rc2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "{dev,eslint-config}@nightly": "link:@remix-run/{dev,eslint-config}@nightly"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
